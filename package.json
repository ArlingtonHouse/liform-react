        "prepublish": "npm run clean && npm run build"
    },
    "author": "Nacho Martin <nacho@limenius.com> (http://github.com/nacmartin)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Limenius/liform-react/issues"
    },
    "homepage": "https://limenius.github.io/liform-react/",
    "dependencies": {
        "ajv": "^4.11.2",
        "bootstrap-sass": "^3.3.7",
        "highlight-loader": "^0.7.2",
        "highlight.js": "^9.9.0",
        "html-loader": "^0.4.4",
        "http-client": "^2.2.0",
        "liform-react": "^0.5.15",
        "lodash.forin": "^4.0.1",
        "markdown-loader": "^0.1.7",
        "marked": "^0.3.6",
        "mkdirp": "^0.5.1",
        "react": "^15.4.2",
        "react-dom": "^15.4.2",
        "react-redux": "^5.0.2",
        "react-router": "next",
        "redux": "^3.6.0",
        "redux-form": "^6.4.3"
    },
    "devDependencies": {
        "babel-cli": "^6.6.4",
        "babel-core": "^6.3.15",
        "babel-eslint": "^5.0.0-beta4",
