        "babel-plugin-transform-object-rest-spread": "^6.22.0",
        "babel-preset-es2015": "^6.3.13",
        "babel-preset-react": "^6.3.13",
        "cross-env": "^1.0.7",
        "css-loader": "^0.26.1",
        "eslint": "^1.6.0",
        "eslint-config-rackt": "^1.1.1",
        "eslint-plugin-babel": "^3.0.0",
        "eslint-plugin-react": "^3.11.3",
        "file-loader": "^0.9.0",
        "node-sass": "^4.5.0",
        "rimraf": "^2.4.3",
